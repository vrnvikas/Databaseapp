<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.materialtest.vikaskumar.databaseapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="ANDROID.PERMISSION.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListActivity"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name=".DatabaseList"
            android:label="@string/title_activity_custom_list_activity" >
        </activity>
        <activity
            android:name=".InternalStorage"
            android:label="@string/title_activity_internal_stroage" >
        </activity>
        <activity
            android:name=".DownloadActivity"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name=".Threadingtest"
            android:label="@string/title_activity_threadingtest" >
        </activity>
    </application>

</manifest>
